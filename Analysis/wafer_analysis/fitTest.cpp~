#include <vector>
#include <iostream>

void fitTest()
{
  std::vector<double> x;
  std::vector<double> y;
  int npoints = 20;
  for(int i{0}; i<npoints; ++i)
    {
      x.push_back(i);
      if(i<=(npoints*1.)/4.)
	{
	  y.push_back(10);
	}
      else if(i>(npoints*1./4.))
	{
	  y.push_back(10-i);
	}
    }
}
